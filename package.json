{
  "name": "NUsight",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "repository": "github:NUbots/NUsight2",
  "engines": {
    "node": ">=4.X.X"
  },
  "scripts": {
    "test": "jest",
    "test:ci": "yarn test --ci",
    "test:watch": "yarn test --watch",
    "coverage": "yarn test --coverage",
    "coverage:ci": "yarn test:ci --coverage",
    "tslint": "tslint -c tslint.json 'src/**/*.ts{,x}' --exclude 'src/shared/proto/**'",
    "tslint:fix": "tslint -c tslint.json 'src/**/*.ts{,x}' --exclude 'src/shared/proto/**' --fix",
    "tscheck": "tsc --project tsconfig.json",
    "tscheck:watch": "yarn tsc --watch",
    "protobuf": "node build_scripts/generate_messages.js",
    "postinstall": "yarn protobuf",
    "dev": "yarn start:virtual",
    "dev:debug": "nodemon --inspect ./src/server/dev.ts --virtual-robots",
    "start": "nodemon ./src/server/dev.ts",
    "start:debug": "nodemon --inspect ./src/server/dev.ts",
    "start:virtual": "nodemon ./src/server/dev.ts --virtual-robots",
    "virtual-robots": "nodemon ./src/virtual_robots/run.ts",
    "prod": "ts-node ./src/server/prod.ts",
    "prod:virtual": "ts-node ./src/server/prod.ts --virtual-robots",
    "build": "yarn clean:dist && webpack -p --progress --colors",
    "clean": "yarn clean:dist && yarn clean:coverage",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "heroku-postbuild": "yarn build",
    "prepush": "yarn tslint"
  },
