{
  "name": "NUsight",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "repository": "github:NUbots/NUbots",
  "engines": {
    "node": ">=16.0.0",
    "yarn": "1.x"
  },
  "scripts": {
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:ci": "yarn test --ci --runInBand",
    "test:watch": "yarn test --watch",
    "coverage": "yarn test --coverage",
    "coverage:ci": "yarn test:ci --coverage",
    "eslint": "eslint \"**/*.ts{,x}\" --cache",
    "eslint:fix": "eslint \"**/*.ts{,x}\" --cache --fix",
    "format:check": "prettier --check --cache \"**/*.{ts,tsx,js,jsx,json,css,md,html}\" \"!src/shared/message*\"",
    "format": "prettier --write --cache \"**/*.{ts,tsx,js,jsx,json,css,md,html}\" \"!src/shared/message*\"",
    "tscheck": "tsc --project tsconfig.test.json",
    "tscheck:watch": "yarn tsc --project tsconfig.test.json --watch",
    "protobuf": "node build_scripts/generate_messages.js",
    "postinstall": "yarn protobuf",
    "rollup": "rollup --watch --config server.rollup.config.js --run",
    "rollup:build": "rollup --config server.rollup.config.js",
    "tailwind": "tailwindcss --watch --no-autoprefixer --output ./src/client/tailwind.css",
    "tailwind:build": "tailwindcss --no-autoprefixer --output ./src/client/tailwind.css",
    "dev": "yarn start:virtual",
    "dev:debug": "run-p tailwind \"rollup -- --virtual-robots --inspect {@}\" --",
    "storybook": "storybook dev -p 9001 --no-open",
    "storybook:build": "storybook build -o dist/storybook",
    "storybook:prod": "node .storybook/prod.js",
    "start": "run-p tailwind \"rollup {@}\" --",
    "start:debug": "run-p tailwind \"rollup -- --inspect {@}\" --",
    "start:virtual": "run-p tailwind \"rollup -- --virtual-robots {@}\" --",
    "virtual-robots": "nodemon src/virtual_robots/run.ts",
    "prod": "node dist/prod.js",
    "prod:virtual": "node dist/prod.js --virtual-robots",
    "build": "yarn tailwind:build && yarn rollup:build --environment BUILD:production && vite build",
    "build:dev": "yarn tailwind:build && yarn rollup:build --environment BUILD:development && vite build",
    "build:ci": "yarn tailwind:build && yarn rollup:build --environment BUILD:production,CI && vite build",
    "clean": "yarn clean:dist && yarn clean:coverage",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "heroku-postbuild": "yarn build && yarn storybook:build"
  },
  "resolutions": {
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-run": "^2.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@rollup/plugin-url": "^6.1.0",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/react-vite": "^7.0.6",
    "@types/component-emitter": "1.2.10",
    "@types/compression": "1.7.1",
    "@types/connect-history-api-fallback": "1.3.5",
    "@types/express": "4.17.13",
    "@types/jest": "29.0.0",
    "@types/minimist": "1.2.2",
    "@types/node": "18.7.14",
    "@types/nodemon": "1.19.2",
    "@types/react": "18.0.18",
    "@types/react-color": "3.0.6",
    "@types/react-dom": "18.0.6",
    "@types/react-measure": "2.0.8",
    "@types/react-transition-group": "4.4.5",
    "@types/seedrandom": "3.0.1",
    "@types/serve-favicon": "2.5.3",
    "@types/three": "^0.143.0",
    "@types/throttle-debounce": "2.1.0",
    "@types/xxhashjs": "0.2.2",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "@typescript-eslint/parser": "7.7.1",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "identity-obj-proxy": "3.0.0",
