{
  "name": "NUsight",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "repository": "github:NUbots/NUbots",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "yarn": "1.x"
  },
  "scripts": {
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:ci": "yarn test --ci --runInBand",
    "test:watch": "yarn test --watch",
    "coverage": "yarn test --coverage",
    "coverage:ci": "yarn test:ci --coverage",
    "eslint": "eslint \"**/*.ts{,x}\" --cache",
    "eslint:fix": "eslint \"**/*.ts{,x}\" --cache --fix",
    "format:check": "prettier --check --cache \"**/*.{ts,tsx,js,cjs,jsx,json,css,md,html}\" \"!src/shared/message*\" \"!dist\"",
    "format": "prettier --write --cache \"**/*.{ts,tsx,js,cjs,jsx,json,css,md,html}\" \"!src/shared/message*\" \"!dist\"",
    "tscheck": "tsc",
    "tscheck:watch": "yarn tsc --watch",
    "protobuf": "node build_scripts/generate_messages.js",
    "postinstall": "yarn protobuf",
    "rollup": "rollup --watch --config server.rollup.config.js --run",
    "rollup:build": "rollup --config server.rollup.config.js",
    "tailwind": "tailwindcss --watch --no-autoprefixer --output ./src/client/tailwind.css",
    "tailwind:build": "tailwindcss --no-autoprefixer --output ./src/client/tailwind.css",
    "dev": "yarn start:virtual",
    "dev:debug": "run-p tailwind \"rollup -- --virtual-robots --inspect {@}\" --",
    "storybook": "storybook dev -p 9001 --no-open",
    "storybook:build": "storybook build -o dist/storybook",
    "storybook:prod": "node .storybook/prod.js",
    "start": "run-p tailwind \"rollup {@}\" --",
    "start:debug": "run-p tailwind \"rollup -- --inspect {@}\" --",
    "start:virtual": "run-p tailwind \"rollup -- --virtual-robots {@}\" --",
    "virtual-robots": "nodemon src/virtual_robots/run.ts",
    "prod": "node dist/prod.js",
    "prod:virtual": "node dist/prod.js --virtual-robots",
    "build": "yarn tailwind:build && yarn rollup:build --environment BUILD:production && vite build",
    "build:dev": "yarn tailwind:build && yarn rollup:build --environment BUILD:development && vite build",
    "build:ci": "yarn tailwind:build && yarn rollup:build --environment BUILD:production,CI && vite build",
    "clean": "yarn clean:dist && yarn clean:coverage",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "heroku-postbuild": "yarn build && yarn storybook:build"
  },
  "resolutions": {
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6"
  },
  "devDependencies": {
